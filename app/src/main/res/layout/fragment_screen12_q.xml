            app:layout_constraintVertical_bias="0.949" />

        <TextView
            android:id="@+id/textView62"
            android:layout_width="131dp"
            android:layout_height="56dp"
            android:layout_marginTop="5dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center|start"
            android:text="8pm-10pm"
            android:textColor="#292929"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="@+id/view162"
            app:layout_constraintEnd_toEndOf="@+id/view162"
            app:layout_constraintHorizontal_bias="0.065"
            app:layout_constraintStart_toStartOf="@+id/view162"
            app:layout_constraintTop_toTopOf="@+id/view162"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView72"
            android:layout_width="131dp"
            android:layout_height="56dp"
            android:layout_marginTop="1dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center|start"
            android:text="10pm-12pm"
            android:textColor="#292929"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="@+id/view152"
            app:layout_constraintEnd_toEndOf="@+id/view152"
            app:layout_constraintHorizontal_bias="0.065"
            app:layout_constraintStart_toStartOf="@+id/view152"
            app:layout_constraintTop_toTopOf="@+id/view152"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView162"
            android:layout_width="131dp"
            android:layout_height="56dp"
            android:layout_marginTop="1dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center|start"
            android:text="12pm or later"
            android:textColor="#292929"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="@+id/view172"
            app:layout_constraintEnd_toEndOf="@+id/view172"
            app:layout_constraintHorizontal_bias="0.043"
            app:layout_constraintStart_toStartOf="@+id/view172"
            app:layout_constraintTop_toTopOf="@+id/view172"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="131dp"
            android:layout_height="56dp"
            android:layout_marginTop="5dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center|start"
            android:text="6pm-8pm"
            android:textColor="#292929"
            android:textSize="23sp"
            app:layout_constraintBottom_toBottomOf="@+id/view112"
            app:layout_constraintEnd_toEndOf="@+id/view112"
            app:layout_constraintHorizontal_bias="0.072"
            app:layout_constraintStart_toStartOf="@+id/view112"
            app:layout_constraintTop_toTopOf="@+id/view112"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView82"
            android:layout_width="345dp"
            android:layout_height="52dp"
            android:layout_marginBottom="371dp"
            android:gravity="center"
            android:text="Thank you *****, Keep Going!"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/view52"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
