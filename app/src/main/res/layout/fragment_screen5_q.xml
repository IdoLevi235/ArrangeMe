            <RadioButton
                android:id="@+id/radioButton11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rarely" />

            <RadioButton
                android:id="@+id/radioButton12"
                android:layout_width="match_parent"
